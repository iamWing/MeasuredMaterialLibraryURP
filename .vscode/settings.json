{
  "omnisharp.useModernNet": false,
  "cSpell.customDictionaries": {
    "unity": {
      "name": "unity",
      "path": "${workspaceFolder}/.vscode/.cspell/unity.txt",
      "addWords": true
    },
    "project": {
      "name": "project",
      "path": "${workspaceFolder}/.vscode/.cspell/project.txt",
      "addWords": true
    }
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.gitmodules": true,
    "**/*.booproj": true,
    "**/*.pidb": true,
    "**/*.suo": true,
    "**/*.user": true,
    "**/*.userprefs": true,
    "**/*.unityproj": true,
    "**/*.exe": true,
    "**/*.pdf": true,
    "**/*.3ds": true,
    "**/*.3DS": true,
    "**/*.fbx": true,
    "**/*.FBX": true,
    "**/*.lxo": true,
    "**/*.LXO": true,
    "**/*.ma": true,
    "**/*.MA": true,
    "**/*.obj": true,
    "**/*.OBJ": true,
    "**/*.asset": true,
    "**/*.cubemap": true,
    "**/*.flare": true,
    "**/*.mat": true,
    "**/*.meta": true,
    "**/*.prefab": true,
    "**/*.unity": true,
    "build/": true,
    "Build/": true,
    "Library/": true,
    "library/": true,
    "obj/": true,
    "Obj/": true,
    "ProjectSettings/": true,
    "temp/": true,
    "Temp/": true
  }
}
